        .txttelefone1.Value = PaginaInicial.LV1.SelectedItem.SubItems(9)
        .txttelefone2.Value = PaginaInicial.LV1.SelectedItem.SubItems(10)
        .txtcep.Value = PaginaInicial.LV1.SelectedItem.SubItems(11)
        .txtlogradouro.Value = PaginaInicial.LV1.SelectedItem.SubItems(12)
        .txtnumero.Value = PaginaInicial.LV1.SelectedItem.SubItems(13)
        .txtcomplemento.Value = PaginaInicial.LV1.SelectedItem.SubItems(14)
        .txtbairro.Value = PaginaInicial.LV1.SelectedItem.SubItems(15)
        .txtcidade.Value = PaginaInicial.LV1.SelectedItem.SubItems(16)
        .txtestado.Value = PaginaInicial.LV1.SelectedItem.SubItems(17)
        .txtreu.Value = PaginaInicial.LV1.SelectedItem.SubItems(18)
        .txtcnpjreu.Value = PaginaInicial.LV1.SelectedItem.SubItems(19)
        .txttelefonereu.Value = PaginaInicial.LV1.SelectedItem.SubItems(20)
        .txtcepreu.Value = PaginaInicial.LV1.SelectedItem.SubItems(21)
        .txtlogradouroreu.Value = PaginaInicial.LV1.SelectedItem.SubItems(22)
        .txtnumeroreu.Value = PaginaInicial.LV1.SelectedItem.SubItems(23)
        .txtcomplementoreu.Value = PaginaInicial.LV1.SelectedItem.SubItems(24)
        .txtbairroreu.Value = PaginaInicial.LV1.SelectedItem.SubItems(25)
        .txtcidadereu.Value = PaginaInicial.LV1.SelectedItem.SubItems(26)
        .txtestadoreu.Value = PaginaInicial.LV1.SelectedItem.SubItems(27)
        .txtprocesso.Value = PaginaInicial.LV1.SelectedItem.SubItems(28)
        .cboxtipoprocesso.Value = PaginaInicial.LV1.SelectedItem.SubItems(29)
        .txtidprocesso.Value = PaginaInicial.LV1.SelectedItem.SubItems(30)
        .cboxstatusprocesso.Value = PaginaInicial.LV1.SelectedItem.SubItems(31)
        .txtnatprocesso.Value = PaginaInicial.LV1.SelectedItem.SubItems(32)
        .txtassunto1.Value = PaginaInicial.LV1.SelectedItem.SubItems(33)
        .txtassunto2.Value = PaginaInicial.LV1.SelectedItem.SubItems(34)
